<br>

<div class="burger-row">
 <div class="burgerAdd">
      
      <h2>Add a Burger Here</h2>
      <form class="create-form">

        <div class="form-group-input">
          <label for="ca">Add your Own Burger:</label>
          <input type="text" id="ca" class="inputBurger" name="name">
        </div>

        <div class="burderAdd2">
          <div class="form-group">
            <label for="eaten"> Have you eaten it or not?</label><br>
         </div> 
            <form action="#">
              
              <p>
                <label>
                  <input name="group1" type="radio" value="0" />
                  <span>Haven't Eat it</span>
                </label>
            </p>

              <p>
                <label>
                  <input name="group1" type="radio" value="1" checked />
                  <span>Eaten It </span>
                </label>
              </p>
      <br>
        

        
          </form>
            {{!-- <input type="radio" name="eaten" value="1" checked> It's Eaten!<br>
            <input type="radio" name="eaten" value="0"> It's Not Eaten<br> --}}


            <button class="btn waves-effect waves-light" type="submit" name="action">Add This Burger</button>
              </div>
        </form>

  </div>



<div class="notEatenBurgers">
    <h2>Burgers Not Eaten...</h2>

    <ul class="burgerList">
      {{#each burgers}}
        {{#unless eaten}}
          {{> burgers/burger-block eaten=true}}
        {{/unless}}
      {{/each}}
    </ul>
</div>

<div class="eatenBurgers">
  <h2>Eaten Burgers!</h2>

  <ul class="burgerList">
    {{#each burgers}}
      {{#if eaten}}
        {{> burgers/burger-block eaten=false}}
      {{/if}}
    {{/each}}
  </ul>
  </div>

</div>

 


